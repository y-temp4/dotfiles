{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.startupEditor": "newUntitledFile",
  "css.validate": false,
  "scss.validate": false,
  "[css]": {
    "editor.tabSize": 2
    // "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[postcss]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "octref.vetur",
    "togglequotes.chars": ["\"", "'", "`"]
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[slim]": {
    "editor.tabSize": 2
  },
  "[erb]": {
    "editor.tabSize": 2
  },
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.renderWhitespace": "all",
  "search.exclude": {
    "**/node_modules": true,
    "**/vendor/bundle": true,
    "**/tmp/cache": true
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html",
    "erb": "html"
  },
  "emmet.syntaxProfiles": {
    "html": {
      "attr_quotes": "double"
    },
    "jsx": {
      "self_closing_tag": true
    },
    "vue": "css"
  },
  "files.insertFinalNewline": true,
  "[solidity]": {},
  "files.associations": {
    "*.css": "postcss"
  },
  "javascript.validate.enable": false,
  "todohighlight.isEnable": true,
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss",
    "**/*.vue"
  ],
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "explorer.confirmDragAndDrop": false,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.rulers": [80, 100],
  "terminal.integrated.fontFamily": "Ricty",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "bold",
  "vetur.experimental.templateInterpolationService": true,
  "liveshare.featureSet": "insiders",
  "diffEditor.ignoreTrimWhitespace": false,
  "vetur.useWorkspaceDependencies": true,
  "stylelint.autoFix": true,
  "postfix.languages": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue"
  ],
  "workbench.sideBar.location": "left",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "vscode-icons",
  "workbench.editor.focusRecentEditorAfterClose": false,
  "tabnine.experimentalAutoImports": true,
  "vetur.format.defaultFormatter.html": "prettier",
  "editor.formatOnSave": true,
  "python.languageServer": "Microsoft",
  "python.showStartPage": false,
  "change-case.default": [
    "snake_case",
    "camelCase",
    "PascalCase",
    "CONST_CASE",
    "param-case"
  ],
  "change-case.ts": [
    "snake_case",
    "camelCase",
    "PascalCase",
    "CONST_CASE",
    "param-case"
  ],
  "git.autofetch": true,
  "extensions.ignoreRecommendations": false,
  "editor.columnSelection": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [],
  "editor.accessibilitySupport": "off",
  "go.toolsManagement.autoUpdate": true,
  "cSpell.customDictionaries": {
    "custom-words": {
      "name": "my-words", // name of dictionary (should be unique)
      "description": "These are the words I use in all projects.",
      "path": "~/.cSpell/custom-words.txt",
      "addWords": true // Add Word to User Dictionary will add words to this file.
    }
  },
  "vscode-edge-devtools.mirrorEdits": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "workbench.colorTheme": "Monokai"
}
